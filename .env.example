# Azure OpenAI Configuration
# Copy this file to .env and fill in your actual values

# Your Azure OpenAI endpoint (e.g., https://your-resource-name.openai.azure.com/ or https://your-resource-name.cognitiveservices.azure.com/)
AZURE_OPENAI_ENDPOINT=

# Your Azure OpenAI deployment name
AZURE_OPENAI_DEPLOYMENT=

# Your Azure OpenAI API key
OPENAI_API_KEY=

# Azure OpenAI API version (optional, defaults to 2024-02-15-preview)
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Model name for display/telemetry purposes (optional, defaults to gpt-4o)
MODEL_NAME=gpt-4o
